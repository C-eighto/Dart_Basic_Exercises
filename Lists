-- Lists

void main() {

  var desserts = ["galletas","panquecitos","gelatinas"];
  
  print("${desserts.length}"); //3
  print("${desserts.first}"); //galletas
  print("${desserts.last}"); //gelatinas
  print("${desserts.isEmpty}");  //false
  print("${desserts.isNotEmpty}"); //true
  
}
